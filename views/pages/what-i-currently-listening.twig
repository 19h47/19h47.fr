{% extends 'index.twig' %}

{# Last.fm #}
{% block start_wrapper %}
	<div class="Lastfm margin-xs-top-64 margin-xs-bottom-64">
{% endblock %}

{% block header %}
	{% include 'pages/_header.twig' %}
{% endblock %}

{% block content %}
	
	<section class="row margin-xs-top-32 margin-xs-bottom-32">
		<div class="col-xs-12">
			<div class="response"></div>
		</div>
	</section>

{% endblock %}

{% block footer %}
	<footer class="row">
		<div class="col-xs-12 col-md-10 col-md-offset-1">
